{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "removeComments": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "allowUnusedLabels": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}